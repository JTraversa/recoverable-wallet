{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"strict": true,
		"noEmit": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"lib": [ "es2018" ],
		"typeRoots": [ "../node_modules/@types", "./typings" ],
	},
}
